{

  "manifest_version": 2,
  "name": "BrowserRelief",
  "version": "1.0",


  "description": "It records the tabs opening now and help you re-open them when Chrome restarted.",
  "icons": {
    "128": "BrowserRelief_128.png"
  },


  "browser_action": {   
    "default_icon": "BrowserRelief_128.png",
    "default_popup": "popup.html"},
          


  "author": "Frank Bai",


  "permissions": [
    "tabs",
    "activeTab",
    "storage"
    
  ]
}